{
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "quoteProps": "consistent",
        "printWidth": 80,
        "arrowParens": "always",
        "trailingComma": "es5"
      }
    }
  ]
}
